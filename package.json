{
	"name": "servido",
	"description": "Versatile services for React ⚗️",
	"version": "0.1.2",
	"license": "MIT",
	"author": "Ludvig Aldén <ludvig@tifum.com>",
	"repository": "ludvigalden/servido",
	"keywords": [
		"react",
		"react-native",
		"service",
		"context",
		"shared",
		"common",
		"id",
		"identified",
		"utility",
		"modules",
		"state",
		"ssr",
		"redux",
		"mobx",
		"observable",
		"solid"
	],
	"main": "dist/index.js",
	"umd:main": "dist/servido.umd.production.min.js",
	"module": "dist/servido.esm.js",
	"typings": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"start": "cross-env NODE_ENV=development tsdx watch --verbose",
		"build": "cross-env NODE_ENV=production tsdx build",
		"test": "tsdx test",
		"lint": "tsdx lint",
		"precommit": "lint-staged",
		"prepublishOnly": "npm run build",
		"publish-canary": "lerna version prerelease --preid canary --force-publish && release --pre",
		"publish-stable": "lerna version --force-publish && release && node ./scripts/release-notes.js",
		"prebuild": "rimraf dist",
		"addc": "all-contributors add",
		"gen-docs": "all-contributors generate && doctoc README.md",
		"git-reset": "git reset --hard HEAD",
		"size": "size-limit"
	},
	"peerDependencies": {
		"react": ">=16.8.0"
	},
	"resolutions": {
		"**/typescript": "4.2.2",
		"**/@typescript-eslint/eslint-plugin": "4.16.1",
		"**/@typescript-eslint/parser": "4.16.1"
	},
	"devDependencies": {
		"@babel/core": "^7.13.8",
		"@babel/preset-env": "^7.13.9",
		"@babel/preset-react": "^7.12.13",
		"@babel/preset-typescript": "^7.13.0",
		"@size-limit/preset-small-lib": "^4.9.2",
		"@types/atob": "^2.1.2",
		"@types/jest": "^25.1.3",
		"@types/react": "^16.9.23",
		"@types/react-test-renderer": "^16.9.2",
		"@typescript-eslint/eslint-plugin": "4.16.1",
		"@typescript-eslint/parser": "4.16.1",
		"all-contributors-cli": "^4.4.0",
		"babel-eslint": "10.1.0",
		"babel-jest": "^25.1.0",
		"cross-env": "^7.0.3",
		"doctoc": "^1.3.0",
		"eslint": "7.18.0",
		"eslint-config-prettier": "7.2.0",
		"eslint-import-resolver-typescript": "2.3.0",
		"eslint-plugin-babel": "5.3.1",
		"eslint-plugin-import": "2.22.1",
		"eslint-plugin-prettier": "3.3.1",
		"got": "^10.6.0",
		"husky": "^4.2.3",
		"jest": "^25.1.0",
		"lerna": "^3.20.2",
		"lint-staged": "^10.0.8",
		"prettier": "2.2.1",
		"react": "^16.8.0",
		"react-test-renderer": "^16.13.0",
		"release": "6.1.0",
		"size-limit": "^4.3.1",
		"ts-node": "9.1.1",
		"tsdx": "0.14.1",
		"typescript": "4.2.2"
	},
	"dependencies": {
		"atob": "^2.1.2",
		"use-cleared-memo": "1.2.0"
	}
}
